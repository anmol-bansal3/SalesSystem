CREATE TABLE `SalesSystem`.`TBL_SALES` ( `id` INT NOT NULL AUTO_INCREMENT , `salesID` VARCHAR(128) NOT NULL , `productID` VARCHAR(128) NOT NULL , `salesDate` DATE NOT NULL , `quantitySold` INT NOT NULL , `salesPricePerUnit` DOUBLE NOT NULL , PRIMARY KEY (`id`)) ENGINE = InnoDB;
CREATE TABLE `SalesSystem`.`TBL_SALES_REPORT` ( `id` INT NOT NULL AUTO_INCREMENT , `salesID` VARCHAR(128) NOT NULL , `productID` VARCHAR(128) NOT NULL , `productName` VARCHAR(128) NOT NULL , `salesDate` DATE NOT NULL , `quantitySold` INT NOT NULL , `productUnitPrice` DOUBLE NOT NULL , `salesPricePerUnit` DOUBLE NOT NULL , `profitAmount` DOUBLE NOT NULL , PRIMARY KEY (`id`)) ENGINE = InnoDB;
